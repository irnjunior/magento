                <value id="getbutton" type="host">https://static.getbutton.io</value>
                <value id="doubleclick" type="host">https://stats.g.doubleclick.net</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
            	<value id="flyout" type="host">https://cdn.minhaloja.com.br</value>
            	<value id="fontawesome" type="host">https://use.fontawesome.com</value>
                <value id="facebook" type="host">https://www.facebook.com</value>
                <value id="facebookconnect" type="host">https://connect.facebook.net</value>
                <value id="facebook" type="host">*.facebook.com</value>
                <value id="googleanalytics" type="host">https://www.google-analytics.com</value>
                <value id="addthis" type="host">https://s7.addthis.com/</value>
                <value id="getbutton" type="host">https://widget.getbutton.io</value>
            </values>
        </policy>
           
        <policy id="font-src">
            <values>
                <value id="data" type="host">'self' data:</value>
                <value id="flyout" type="host">https://cdn.minhaloja.com.br</value>
                <value id="cloudflare" type="host">*.cloudflare.com</value>
                <value id="twitter.com" type="host">*.twitter.com</value>
                <value id="google.com" type="host">*.google.com</value>
                <value id="google.co.in" type="host">*.google.co.in</value>
                <value id="facebook.com" type="host">*.facebook.com</value>
                <value id="gstatic" type="host">*.gstatic.com</value>
                <value id="typekit" type="host">*.typekit.net</value>
                <value id="twimg" type="host">*.twimg.com</value>
                <value id="trustedshops" type="host">*.trustedshops.com</value>
                <value id="googleapis" type="host">*.googleapis.com</value>
                <value id="bootstapcdn" type="host">*.bootstrapcdn.com</value>
                <value id="addthis" type="host">https://s7.addthis.com/</value>
                <value id="googleanalytics" type="host">https://www.google-analytics.com</value>
            </values>
        </policy>
        <policy id="form-action">
            <values>
                <value id="facebookconnect" type="host">https://connect.facebook.net</value>
                <value id="facebook.com" type="host">*.facebook.com</value>
                <value id="googleanalytics" type="host">https://www.google-analytics.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>