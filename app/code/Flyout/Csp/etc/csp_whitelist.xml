<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>    
            	<value id="flyout" type="host">https://cdn.minhaloja.com.br</value>        	
            	<value id="cloudfront" type="host">*.cloudfront.net</value>
                <value id="fontawesome" type="host">https://use.fontawesome.com</value>
                <value id="facebook" type="host">https://www.facebook.com</value>
                <value id="facebookconnect" type="host">https://connect.facebook.net</value>
                <value id="facebook" type="host">*.facebook.com</value>
                <value id="googleanalytics" type="host">https://www.google-analytics.com</value>   
                <value id="cloudflare" type="host">cloudflare.com</value>
                <value id="magentocommerce" type="host">*.widgets.magentocommerce.com</value>   
                <value id="addthis" type="host">https://s7.addthis.com/</value>                
            </values>
        </policy>
        <policy id="script-src">
